
#include<stdio.h>
void main()
{
    // Swapping of two variables;
    
    int var1, var2;
    printf("Enter values for two variables : ");
    scanf("%d %d",&var1,&var2);
    
    printf("Values before swapping : %d %d \n",var1,var2);

    int temp;
    temp = var1;            // Using temp variable;
    var1 = var2;
    var2 = temp;
    
    printf("Values after swapping : %d %d",var1,var2);
}
