#include <stdio.h>
void main()
{
    // Fibonacci Series;

    int N;
    printf("Enter the Nth value : ");
    scanf("%d", &N);

    int n1 = 0;
    int n2 = 1;
    int n3 = 0;
    printf("The fibonacci series follows : %d\t%d",n1,n2);

    for (int i=3;i<=N;i++)
    {
        n3 = n1 + n2;
        n1 = n2;
        n2 = n3;
        printf ("\t%d",n3);
    }
}
