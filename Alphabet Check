
#include<stdio.h>
void main()
{
    // Alphabet or Not;
    
    char test;
    printf("Enter a character : ");
    scanf("%c",&test);
    
    if (((test >= 65) && (test <= 90)) || ((test >= 97) && (test <= 122)))
        printf("Its an Alphabet!");
    else
        printf("Its not an alphabet");
}
