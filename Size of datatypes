
#include<stdio.h>
void main()
{
    char n1;
    int n2;
    float n3;
    double n4;
    
    printf("\n Size of char is  : %d", sizeof(n1));
    printf("\n Size of int is   : %d", sizeof(n2));
    printf("\n Size of float is : %d", sizeof(n3));
    printf("\n Size of double is: %d", sizeof(n4));
}
