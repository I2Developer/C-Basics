
#include<stdio.h>
void main()
{
    char test;
    printf("Enter a character : ");
    scanf("%c",&test);
    
    if (test == 'a' || test == 'e' || test == 'i'  || test == 'o'  || test == 'u' || test == 'A' || test == 'E' || test == 'I' || test == 'O'   || test == 'U')
        printf("Its a Vowel!");
    else
        printf("Its not a vowel");
}
