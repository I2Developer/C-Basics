
#include<stdio.h>
void main()
{
    // ASCII value of Alphabet;
    
    char test;
    printf("Enter a character : ");
    scanf("%c",&test);
    
    if (((test >= 65) && (test <= 90)) || ((test >= 97) && (test <= 122)))
        printf("ASCII value of %c is : %d", test, test);
    else
        printf("Enter valid alphabet");
}
